package com.cicidi.generic;

import java.util.Arrays;

public class GenericTest {
	public static void main(String[] arg) {
		int[] intArray = { 1, 23, 82, 83, 13, 25 };
		double[] doubleArray = { 1, 23, 82, 83, 13, 25 };
		getResult(intArray);
	}

	public static <T> void getResult(T[] array) {
		Arrays.sort(array);
		System.out.println(array);
	}
}
