package com.cicidi.time.apple_screen.test;

import static org.junit.Assert.*;

import org.junit.Test;

import com.cicidi.time.apple_screen.FightServeTime;

public class FightServeTimeTest {

	@Test
	public void testStart() {
		FightServeTime action = new FightServeTime();
		Integer[] array = { 3, 4, 5, 7, 10, 13, 18, 20, 24, 25 };
		action.init(array);
		action.start(action.currentRow);
		System.out.println("Total Spend time is "
				+ (action.currentTime - action.startTime) + "seconds");
	}
}
