package com.cicidi.multithread.notify;

public class Test {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		Thread thread1 = new Thread(new ThreadMgt("Hello"));
		Thread thread2 = new Thread(new ThreadMgt("World"));
		thread1.start();
		thread1.wait();
		thread2.start();
		thread2.notifyAll();
	}
}
