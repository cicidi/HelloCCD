package com.cicidi.multithread;

public class HelloWorld {
	public static void main(String[] arg) {
		Thread thread1 = new Thread() {
			public void run() {
				System.out.print("Hello");
			}
		};

		Thread thread2 = new Thread() {
			public void run() {
				System.out.print("World");
			}
		};
	}
}