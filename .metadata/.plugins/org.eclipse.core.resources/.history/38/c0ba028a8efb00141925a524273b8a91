package com.cicidi.time.apple_screen.test;

import static org.junit.Assert.assertEquals;

import org.junit.Test;

import com.cicidi.time.apple_screen.FightServeTime;

public class FightServeTimeTest {

	@Test
	public void testStart() {
		FightServeTime action = new FightServeTime();
		Integer[] array1 = { 3 };
		Integer[] array2 = { 3, 4 };
		Integer[] array3 = { 3, 4, 5 };
		Integer[] array4 = { 3, 4, 5, 7 };
		Integer[] array5 = { 3, 4, 5, 7, 10 };
		Integer[] array6 = { 3, 4, 5, 7, 10, 13 };
		Integer[] array7 = { 3, 4, 5, 7, 10, 13, 20 };
		Integer[] array8 = { 3, 4, 5, 7, 10, 13, 20, 24 };
		Integer[] array9 = { 3, 4, 5, 7, 10, 13, 20, 24, 25 };
		action.init(array1);
		int result1 = action.start(action.currentRow);
		action.init(array2);
		int result2 = action.start(action.currentRow);
		action.init(array2);
		int result3 = action.start(action.currentRow);
		action.init(array2);
		int result4 = action.start(action.currentRow);
		action.init(array2);
		int result5 = action.start(action.currentRow);
		action.init(array2);
		int result6 = action.start(action.currentRow);
		action.init(array2);
		int result7 = action.start(action.currentRow);
		action.init(array2);
		int result8 = action.start(action.currentRow);
		action.init(array2);
		int result9 = action.start(action.currentRow);
		action.init(array2);
		int result10 = action.start(action.currentRow);
		int[] result = { result1, result2, result3, result4, result5, result6,
				result7, result8, result9, result10 };
		// System.out.p
		int[] expect = { 7, 12, 17, 24, 33, 42, 59, 138, 143 };
		assertEquals(expect, result);

	}
}
