package com.cicidi.multithread;

public class HelloWorld2 implements Runnable {
	private Thread thread;
	private String threadMsg;

	public HelloWorld2(String threadMsg) {
		// TODO Auto-generated constructor stub
		this.thread = new Thread();
		this.threadMsg = threadMsg;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println(threadMsg);
	}

	public void start() {
		this.run();
	}

}
