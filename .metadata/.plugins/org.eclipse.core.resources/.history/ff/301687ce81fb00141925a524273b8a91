package com.cicidi.time.apple_screen.test;

import static org.junit.Assert.*;

import org.junit.Test;

import com.cicidi.time.apple_screen.FightServeTime;

public class FightServeTimeTest {

	@Test
	public void testStart() {
		FightServeTime action = new FightServeTime();
		Integer[] array1 = { 3 };
		Integer[] array2 = { 3, 4 };
		Integer[] array3 = { 3, 4, 5 };
		Integer[] array4 = { 3, 4, 5, 6 };
		Integer[] array5 = { 3, 4, 5, 6, 7 };
		Integer[] array5 = { 3, 4, 5, 6, 7, 8 };
		Integer[] array6 = { 3, 4, 5, 6, 7, 8, 9, 10 };
		Integer[] array6 = { 3, 4, 5, 6, 7, 8, 9, 10, 13 };
		Integer[] array2 = { 3, 4, 5, 7, 10, 13, 18, 20, 24, 25 };
		action.init(array1);
		action.start(action.currentRow);
		int result1 = action.start(action.currentRow);
		Integer[] array2 = { 3, 4, 5, 7, 10, 13, 18, 20, 24, 25 };
		action.init(array2);
		int result2 = action.start(action.currentRow);
	}
}
