package com.cicidi.multithread.notify;

public static class ThreadLocalMgt {
	public static final ThreadLocal<String> threadLocal;
	static {
		threadLocal = new ThreadLocal<String>();
	}

	public static void set(String msg) {
		threadLocal.set(msg);
	}

	public static String get() {

		return (String) threadLocal.get();
	}
}
